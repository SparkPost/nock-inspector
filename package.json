{
  "name": "@sparkpost/nock-inspector",
  "version": "0.2.0",
  "description": "A wrapper for nock that exposes request data",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "tsc && NODE_ENV=test NODE_CONFIG_PERSIST_ON_CHANGE=N NODE_PATH=$NODE_PATH:. nyc --reporter=text-summary --reporter=lcov --report-dir=test/report/ mocha test/",
    "lint": "eslint ./**/*.{js,ts} --fix",
    "tsc": "tsc"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "nock": "^12.0.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/lodash.isequal": "^4.5.5",
    "@types/nock": "^11.1.0",
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "bluebird": "^3.7.2",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-sparkpost": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.15",
    "mocha": "^5.2.0",
    "nyc": "^15.0.0",
    "prettier": "^2.0.4",
    "prettier-eslint-cli": "^5.0.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier-eslint --write",
      "eslint --fix"
    ],
    "**/*.ts": [
      "eslint --fix"
    ]
  }
}
